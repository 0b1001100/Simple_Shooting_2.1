{
  "java.project.referencedLibraries": [
    "lib/**/*.jar",
    "antlr-4.10.1-complete.jar",
    "antlr-4.11.1-complete.jar"
  ],
  "antlr4.generation":{
    "mode": "external",   // 決め打ち(多分)
    "language": "Java", // 出力したいプログラミング言語名
    "listeners": true,    // listener形式で出力するか
    "visitors": true,    // visitor形式で出力するか
    "alternativeJar": "antlr-4.11.1-complete.jar", // ANTLRのjarファイル(※)
    "outputDir": ".antlr/com/parser/command" // 出力先のフォルダ名
  }
}